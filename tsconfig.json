{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "typeRoots": ["node_modules/@types"],
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "module": "CommonJS",
    "pretty": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "./dist",
    "allowJs": true,
    "noEmit": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "baseUrl": "src",
    "skipLibCheck": true,
    "paths": {
      "@*": ["*"],
      "@dto/*": ["Dto/*"],
      "@providers/*": ["Providers/*"],
      "@configs/*": ["Config/*"],
      "@sqs/*": ["Sqs/*"],
      "@internal/*": ["Internal/*"],
    }
  },
}
